{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env":{
      "es6":true,
      "browser":true,
      "node":true
    },
    "rules":{
      "quotes":0,
      "no-unused-var":0,
      "comma-spacing":0,
      "comma-dangle":0,
      "arrow-body-style":0,
      "semi":0,
      "react/jsx-filename-extension":0,
      "space-before-blocks":0,
      "class-methods-use-this":0,
      "react/forbid-prop-types":0,
      "block-spacing":0,
      "key-spacing":0,
      "arrow-parens":0,
      "object-curly-spacing":0
    }
}
