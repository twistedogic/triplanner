{
  "name": "triplanner",
  "version": "1.0.0",
  "description": "tsp for trip planning",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack -p --config webpack.prod.config.js",
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "karma": "NODE_ENV=test ./node_modules/karma/bin/karma start karma.conf.js",
    "publish_pages": "./node_modules/.bin/gh-pages -d dist",
    "ghpages": "npm run build && npm run publish_pages",
    "karma:watch": "npm run karma -- --watch",
    "test": "mocha tools/testSetup.js 'src/**/*.test.js'",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twistedogic/triplanner.git"
  },
  "author": "Jordan Li",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/twistedogic/triplanner/issues"
  },
  "homepage": "https://github.com/twistedogic/triplanner#readme",
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "font-awesome": "^4.7.0",
    "normalize.css": "^5.0.0",
    "react": "15.0.2",
    "react-async-script-loader": "^0.2.1",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "react-sortable-hoc": "0.0.9",
    "react-toolbox": "^2.0.0-beta.0",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jsdom": "8.5.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "mocha": "2.4.5",
    "nock": "^9.0.2",
    "npm-run-all": "1.8.0",
    "phantomjs": "^2.1.7",
    "phantomjs-polyfill": "0.0.2",
    "react-addons-test-utils": "15.0.2",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "rimraf": "2.5.2",
    "sinon": "^1.17.6",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
